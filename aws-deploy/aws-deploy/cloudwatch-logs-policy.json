{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "logs:DescribeLogStreams"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:*:log-group:/ecs/poker-night-app*",
        "arn:aws:logs:us-east-1:*:log-group:/ecs/poker-night-app*:log-stream:*"
      ]
    }
  ]
}
